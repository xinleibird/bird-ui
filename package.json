{
  "private": true,
  "name": "dumi-lib",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "build": "npm run docs:build",
    "build:father": "father-build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "clean": "rimraf dist",
    "test": "umi-test --passWithNoTests",
    "test:coverage": "umi-test --coverage --passWithNoTests",
    "deploy": "gh-pages -d dist -r https://github.com/xinleibird/bird-ui.git",
    "push": "npm run docs:build && npm run deploy"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@xinleibird/bird-ui": "^0.1.35",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.7",
    "father-build": "^1.17.2",
    "gh-pages": "^2.2.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
}
