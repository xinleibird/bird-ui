  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:js && npm run build:css && npm run build:asset",
    "build:js": "tsc -p tsconfig.build.json",
    "build:css": "node-sass -r src -o dist --source-map=true --output-style=compressed --linefeed=lf",
    "build:asset": "npx ncp src/components/Icon/icons/assets/ dist/components/Icon/icons/assets/",
    "build:site": "react-scripts build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react-dom": "^16.9.0",
